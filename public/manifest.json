{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "ru",
  "version": "2.0.12",
  "background": {
    "scripts": ["static/js/background.bundle.js"]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "img/icon_128.png"
    }
  },
  "icons": {
    "128": "img/icon_128.png"
  },
  "permissions": [
    "alarms",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}